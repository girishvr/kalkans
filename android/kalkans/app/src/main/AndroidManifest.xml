<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bitjini.kalkans">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="HELPJINI"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SignAupctivity" />
        <activity android:name=".SafetytipsActivity" />
        <activity android:name=".FloodActivity" />
        <activity android:name=".AccidentActivity" />
        <activity android:name=".MedicalEmergencyActivity" />
        <activity android:name=".LandSlidesActivity" />
        <activity android:name=".TheftActivity" />
        <activity android:name=".EarthquakeActivity" />
        <activity android:name=".WomenSafetyActivity" />
        <activity android:name=".TerroristActivity" />
        <activity android:name=".icons_test" />
        <activity android:name="VideoStream" />

    </application>

</manifest>