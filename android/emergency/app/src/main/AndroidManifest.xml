<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.com.emergency">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Accidents" />
        <activity android:name=".Splash" />
        <activity android:name=".Medical_Emergency" />
        <activity android:name=".Theft" />
        <activity android:name=".Flood" />
        <activity android:name=".Land_Slides" />
        <activity android:name=".Terrorist" />
        <activity android:name=".Women_Safety" />
        <activity android:name=".Earthquake"></activity>
    </application>

</manifest>